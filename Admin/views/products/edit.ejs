        <div class="content container-fluid">
            <form class="row g-3 needs-validation px-4 mt-4" action="/products/edit/<%= product.id %>" method="POST" novalidate>
                <div class="col-md-12 position-relative">
                    <label for="validationTooltip01" class="form-label">
                        <%= product.productName %>
                    </label>
                    <input type="text" class="form-control ps-2" id="validationTooltip01" name="productName"
                        value="<%= product.productName %>" required style="border: 1px solid lightgray;">
                    <div class="valid-tooltip">Looks good!</div>
                </div>

                <div class="col-md-4 position-relative">
                    <label for="validationTooltip02" class="form-label">Old Price</label>
                    <input type="text" class="form-control ps-2" id="validationTooltip02" name="oldPrice"
                        value="<%= product.oldPrice %>" style="border: 1px solid lightgray;">
                    <!-- <div class="invalid-tooltip">Please provide a valid Oldprice.</div> -->
                </div>

                <div class="col-md-4 position-relative">
                    <label for="validationTooltip03" class="form-label">Price</label>
                    <input type="number" class="form-control ps-2" id="validationTooltip03" name="price"
                        value="<%= product.price %>" required style="border: 1px solid lightgray;">
                    <div class="invalid-tooltip">Please provide a valid price.</div>
                </div>

                <!-- <div class="col-md-4 position-relative">
                    <label for="validationTooltip04" class="form-label">
                        Weight (comma separated):
                    </label>
                    <input type="text" class="form-control ps-2" id="validationTooltip04" name="weight" value="<%= product.weight.join(',') %>" required style="border: 1px solid lightgray;">
                    <div class="valid-tooltip">Looks good!</div>
                </div> -->

                <div class="col-md-12 position-relative">
                    <label for="validationTooltip06" class="form-label">Description</label>
                    <input type="text" class="form-control ps-2" id="validationTooltip06" name="description"
                        value="<%= product.description %>" required style="border: 1px solid lightgray;">
                    <div class="invalid-tooltip">Please provide a valid description.</div>
                </div>

                <div class="col-md-4 position-relative">
                    <label for="validationTooltip07" class="form-label">Rating</label>
                    <select class="form-select ps-2" id="validationTooltip07" name="rating" required
                        style="border: 1px solid lightgray;">
                        <option selected disabled value="">Choose...</option>
                        <% [1.0, 1.5, 2.0, 2.5, 3.0, 3.5, 4.0, 4.5, 5.0].forEach(function(rating) { %>
                            <option value="<%= rating %>" <%=product.rating==rating ? 'selected' : '' %>><%= rating %>
                            </option>
                            <% }); %>
                    </select>
                    <div class="invalid-tooltip">Please select a valid rating.</div>
                </div>

                <div class="col-md-4 position-relative">
                    <label for="validationTooltip09" class="form-label">Discount:</label>
                    <input type="text" class="form-control ps-2" id="validationTooltip09" name="discount" value="<%= product.discount %>" required style="border: 1px solid lightgray;">
                </div>

                <div class="col-md-4 position-relative">
                    <label for="validationTooltip10" class="form-label">Category</label>
                    <input type="text" class="form-control ps-2" id="validationTooltip10"  name="category" value="<%= product.category %>" required style="border: 1px solid lightgray;">
                </div>
                

                <div class="col-md-6 position-relative">
                    <label for="validationTooltip07" class="form-label">Category Image URL:</label>
                    <input type="text" class="form-control ps-2" id="validationTooltip07" name="catImg" value="<%= product.catImg %>" required style="border: 1px solid lightgray;">
                    <div class="invalid-tooltip">Please provide a valid price.</div>
                </div>

                <div class="col-md-6 position-relative">
                    <label for="validationTooltip08" class="form-label">Product Images (comma separated)</label>
                    <input type="text" class="form-control ps-2" id="validationTooltip08" name="productImages" value="<%= product.productImages.join(',') %>" required style="border: 1px solid lightgray;">
                    <div class="invalid-tooltip">Please provide valid product images URLs, separated by commas.</div>
                </div>

                <div class="col-12">
                    <button class="btn btn-success" type="submit">Update Product</button>
                </div>
            </form>
